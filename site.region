<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="initial-scale=1">

    <!-- above the fold/critical css -->
    {@|apply critical.block}

    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div id="site" class="site-container clearfix">

      <header>

        <squarespace:navigation navigationId="primary-nav" template="navigation" />
      </header>

      <main id="page" class="site-page site-page--{collection.typeName}{.equal? collection.typeName "page"}{.or}-{.section item}item{.or}list{.end}{.end}" data-content-field="main-content">
        {squarespace.main-content}
      </main>

      <footer>
        <squarespace:block-field id="footer-blocks" columns="12" />
      </footer>

    </div>

    <script type="text/javascript" src="/scripts/site-bundle.js"></script>

    {squarespace-footers}

  </body>
</html>
